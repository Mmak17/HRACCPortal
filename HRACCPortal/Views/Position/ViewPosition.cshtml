    <script>
        new tempusDominus.TempusDominus(document.getElementById("InactiveDate"), {
            display: {
                viewMode: "calendar",
                components: {
                    decades: true,
                    year: true,
                    month: true,
                    date: true,
                    hours: false,
                    minutes: false,
                    seconds: false
                }
            }
        });

        function EditPosition_Click(clicked) {
            $.ajax({
                type: "POST",
                url: '/Position/EditPosition',
                data: '{id: ' + clicked + '}',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (response) {
                    $("#btnpopup").click();
                    //$("input[name='CustomerName']").val(response.positionData.CustomerName);
                    $(".CustomerName").val(response.positionData.CustomerIdFK);
                    $(".CustomerName").change();
                    $("input[name='PositionIdPK']").val(response.positionData.PositionIdPK);
                    $("input[name='CustomerIdFK']").val(response.positionData.CustomerIdFK);
                    $("input[name='PositionNumber']").val(response.positionData.PositionNumber).attr("disabled", "disabled");
                    $("input[name='PositionTitle']").val(response.positionData.PositionTitle);
                    $("input[name='PositionFamily']").val(response.positionData.PositionFamily);
                    $("input[name='PositionScopeVariant']").val(response.positionData.PositionScopeVariant);
                    $("input[name='PurchaseOrderNo']").val(response.positionData.PurchaseOrderNo).addClass('input-disabled');
                    $("input[name='PurchaseRequisitionNo']").val(response.positionData.PurchaseRequisitionNo);

                    if (response.positionData.Status == true) {
                        $("#Status").prop('checked', true);
                        $("#hide_inactivedetails").hide();
                    }
                    if (response.positionData.Status == false) {
                        $("#Status").prop('checked', false);
                        $("#hide_inactivedetails").show();
                    }

                    $("input[name='InactiveDate']").val(response.positionData.InactiveDate);
                    $("input[name='InactiveReason']").val(response.positionData.InactiveReason);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    submitButton.removeAttribute('data-kt-indicator');

                    console.log("Custom error : " + jqXHR.responseText + " Status: " + textStatus + " Http error:" + errorThrown);

                    Swal.fire({
                        text: "Custom error : " + jqXHR.responseText + " Status: " + textStatus + " Http error:" + errorThrown,
                        icon: "error",
                        buttonsStyling: false,
                        confirmButtonText: "Ok, got it!",
                        customClass: {
                            confirmButton: "btn btn-primary"
                        }
                    });

                }
            });

           
           
        }
        $(document).ready(function () {
            $("#Status").click(function () {
                if ($(this).is(":checked")) {

                    $("#hide_inactivedetails").hide();
                    $("#divreason").hide();
                    $("#divdate").hide();
                }
                else {

                    $("#hide_inactivedetails").show();
                    $("#divreason").show();
                    $("#divdate").show();
                }
            });
        });
         $("#InactiveReason").blur(function () {
                if ($(this).val() == "") {
                    $("#divreason").show();
                } else {
                    $("#divreason").hide();
                }
            });

            $("#InactiveDate").blur(function () {
                if ($(this).val() == "") {
                    $("#divdate").show();
                } else {
                    $("#divdate").hide();
                }
            });
    </script>
}
